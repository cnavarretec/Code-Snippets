import pandas as pd

myfile="C:\\Users\\ramshj1\\Box Sync\\My Data\\_Projects\\Data Mining\\DOC Data\\Composite Parts List - 10MAY2018.xlsx"
df = pd.read_excel(open(myfile,'rb'), sheet_name="updated doc")

# return series containing all duplicates
df[df.duplicated(['ID'], keep=False)]

# alternative 
mask = df.MyColumnName.duplicated(keep=False)
df_duplicates=df[mask]
df_notduplicates=df[~mask]
